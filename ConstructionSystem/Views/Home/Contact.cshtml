

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    @*/*********************bootstrap css*********************************/*@

    <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    @*/*********************bootstrap JS*********************************/

        <script src="~/bootstrap/js/bootstrap.min.js"></script>*@

    @*/*********************Fontawesome Css*********************************/*@
    <link href="~/fontawesome-free-5.11.2-web/css/all.css" rel="stylesheet" />

    @*/*********************Fontawesome JS*********************************/*@
    <script src="~/fontawesome-free-5.11.2-web/js/all.min.js"></script>

    @*/*********************css*********************************/*@

    <link href="~/Project Css/Contact.css" rel="stylesheet" />
    <link rel="stylesheet" href="bootstrap-4.1.3/dist/css/bootstrap.min.css">



    <title>Contact</title>
</head>
<body>
    @section UserNav{
        @{ Html.RenderPartial("_UserNav");}
    }
    <section class="container-fluid">
        <div id="map-container-google-1" class="z-depth-1-half map-container" style="height: 500px">
            <iframe src="https://maps.google.com/maps?q=manhatan&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0"
                    style="border:0;width:100%;height:100%" allowfullscreen></iframe>
        </div>
    </section>
    <section class="login-block">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <form class="md-float-material form-material form-group">
                        <div class="auth-box card">
                            <div class="card-block">
                                <div class="row">
                                    <div class="col-md-12 contactheader">
                                        <h3 class="text-center"><i class="fa fa-phone-square text-primary f-56"></i></h3>
                                        <h3 class="text-center contact-us"><b>Keep In Touch With Us</b></h3>
                                        <h6 class="text-center respond">(we generally respond in 24 hours)</h6>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="form-primary"><label class="label">Name</label> <input type="text" name="email" class="form-control text-left inputName"  required=""> </div>
                                    </div>
                                    <div class="col-sm-12">
                                        <div class="form-primary"><label class="label">Email</label> <input type="text" name="email" class="form-control text-left inputName" required=""> </div>
                                    </div>
                                </div>
                                <div class="form-primary"><label class="label">Message</label> <input type="text" name="email" class="form-control text-left inputmsg"  required=""> </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <button type="button" class="btn btn-dark btn-md btn-block waves-effect text-center m-b-20"><i class="fa fa-phone"></i> Contact Now </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
    <section class=" stylesec">

        <div class="container mt-5">
            <div class="row">
                <div class=" col-md-3 col-sm-6">
                    <div class="card text-white bg-dark mb-3" style="max-width: 18rem;">
                        <div class="card-header">Location</div>
                        <div class="card-body">

                            <p class="card-text">Nasr City 1, Cairo, Egypt.</p>
                        </div>
                    </div>
                </div> 
                <div class="col-md-3 col-sm-6">
                    <div class="card text-white bg-dark mb-3" style="max-width: 18rem;">
                        <div class="card-header">Email</div>
                        <div class="card-body">

                            <p class="card-text">afrina@gmail.com</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="card text-white bg-dark mb-3 " style="max-width: 18rem;">
                        <div class="card-header">Work Time</div>
                        <div class="card-body">
                            <p class="card-text"> 9:00AM - 5:00PM</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="card text-white bg-dark mb-3" style="max-width: 18rem;">
                        <div class="card-header">Phone</div>
                        <div class="card-body">

                            <p class="card-text">+201012111262</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </section>
    @*/***********************js*********************/*@

    @*/***********************Jquery*********************/*@

    <script src="~/jquery-3.4.1.js"></script>
    @*/***********************Bootstrap JS*********************/*@


    <script src="~/bootstrap/js/bootstrap.min.js"></script>
</body>
</html>
