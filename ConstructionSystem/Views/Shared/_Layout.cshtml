<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    @Styles.Render("~/Content/css")
    <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/fontawesome-free-5.11.2-web/css/all.css" rel="stylesheet" />
    <link href="~/Project Css/NavFooter.css" rel="stylesheet" />
    @RenderSection("Styles", false)
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark navtransperant homenav" style="color:burlywood ; background-color:#808080; position: fixed; top: 0 !important; left: 0 !important; right: 0 !important; z-index: 999999999999;">
        <img width="60" class="starting-logo dark-version default-logo" alt="Afrina Company" src="https://afrina.net/wp-content/uploads/2019/01/Afrina-Logo-Light-2.svg" srcset="https://afrina.net/wp-content/uploads/2019/01/Afrina-Logo-Light-2.svg 1x, https://afrina.net/wp-content/uploads/2019/01/Afrina-Logo-Light-2.svg 2x" scale="0">
        <button class="navbar-toggler" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarNavDropdown">

            @RenderSection("UserNav",false)
            @RenderSection("AdminNav",false)
            
         
        </div>

    </nav>
    <div>
        @RenderBody()
        <hr />
    </div>
    <footer class="page-footer font-small stylish-color-dark pt-4" style="bottom: 0 !important; left: 0 !important; right: 0 !important; z-index: 999999999999;">

        <!-- Footer Links -->
        <div class="container text-center text-md-left">

            <!-- Grid row -->
            <div class="row">

                <!-- Grid column -->
                <div class="col-md-4 mx-auto">
                    <img width="100" class="starting-logo dark-version default-logo d-sm-flex" alt="Afrina Company" src="https://afrina.net/wp-content/uploads/2019/01/Afrina-Logo-Light-2.svg" srcset="https://afrina.net/wp-content/uploads/2019/01/Afrina-Logo-Light-2.svg 1x, https://afrina.net/wp-content/uploads/2019/01/Afrina-Logo-Light-2.svg 2x" scale="0">
                    <!-- Content -->



                </div>
                <!-- Grid column -->

                <hr class="clearfix w-100 d-md-none">

                <!-- Grid column -->
                <div class="col-md-2 mx-auto">

                    <!-- Links -->
                    <h5 class="font-weight-bold text-uppercase mt-3 mb-4 ">Quick Links</h5>

                    <ul class="list-unstyled">
                        <li>
                            @Html.ActionLink("Who We Are", "About", null, new { @class = "footerlist" })


                        </li>
                        <li>
                            @Html.ActionLink("Projects", "Project", null, new { @class = "footerlist" })

                        </li>
                        <li>
                            @Html.ActionLink("Services", "Service", null, new { @class = "footerlist" })


                        </li>

                    </ul>

                </div>
                <!-- Grid column -->

                <hr class="clearfix w-100 d-md-none">

                <!-- Grid column -->
                <div class="col-md-2 mx-auto">

                    <!-- Links -->
                    <h5 class="font-weight-bold text-uppercase mt-3 mb-4">Quick Links</h5>

                    <ul class="list-unstyled">
                        <li>
                            @Html.ActionLink("Team", "Team", null, new { @class = "footerlist" })

                        </li>
                        <li>
                            @Html.ActionLink("Contact Us", "Contact", null, new { @class = "footerlist" })

                        </li>


                    </ul>

                </div>
                <!-- Grid column -->

                <hr class="clearfix w-100 d-md-none">

                <!-- Grid column -->
                <!-- Grid column -->

            </div>
            <!-- Grid row -->

        </div>
        <!-- Footer Links -->
        <!-- Social buttons -->
        <ul class="list-unstyled list-inline text-center d-sm-none d-md-block d-lg-block">
            <li class="list-inline-item">
                <a href="https://www.facebook.com" class="btn-floating btn-fb mx-1">
                    <i class="fab fa-facebook-f icon"> </i>
                </a>
            </li>
            <li class="list-inline-item">
                <a href="https://twitter.com" class="btn-floating btn-tw mx-1">
                    <i class="fab fa-twitter icon"> </i>
                </a>
            </li>
            <li class="list-inline-item">
                <a href="https://accounts.google.com" class="btn-floating btn-gplus mx-1">
                    <i class="fab fa-google-plus-g icon"> </i>
                </a>
            </li>
            <li class="list-inline-item">
                <a href="https://linkedin.com" class="btn-floating btn-li mx-1">
                    <i class="fab fa-linkedin-in icon"> </i>
                </a>
            </li>
            <li class="list-inline-item">
                <a href="https://dribbble.com" class="btn-floating btn-dribbble mx-1">
                    <i class="fab fa-dribbble icon"> </i>
                </a>
            </li>
        </ul>
        <!-- Social buttons -->
        <!-- Copyright -->
        <div class="footer-copyright text-center py-3 d-sm-none d-md-block d-lg-block">
            © 2018 Copyright:
            <a href="#!" class="AFRINA"> AFRINA.com</a>
        </div>
        <!-- Copyright -->

    </footer>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>
